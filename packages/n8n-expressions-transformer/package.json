{
	"name": "@refluxo/n8n-expressions-transformer",
	"version": "1.0.0",
	"description": "n8n-compatible expressions for Refluxo",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.ts --format cjs,esm --dts --clean",
		"dev": "tsup src/index.ts --format cjs,esm --watch --dts",
		"lint": "tsc --noEmit"
	},
	"keywords": [
		"refluxo",
		"middleware",
		"n8n",
		"expressions",
		"tournament"
	],
	"author": "Vitor Daniel <vitor036daniel@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@n8n/tournament": "^1.0.6",
		"luxon": "^3.5.0",
		"jmespath": "^0.16.0"
	},
	"peerDependencies": {
		"@refluxo/core": "workspace:*"
	},
	"devDependencies": {
		"@refluxo/core": "workspace:*",
		"@types/luxon": "^3.4.2",
		"@types/jmespath": "^0.15.2",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3"
	}
}
