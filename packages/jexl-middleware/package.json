{
	"name": "@refluxo/jexl-middleware",
	"version": "1.0.0",
	"description": "Jexl expression middleware for Refluxo",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.ts --format cjs,esm --dts --clean",
		"dev": "tsup src/index.ts --format cjs,esm --watch --dts",
		"lint": "tsc --noEmit"
	},
	"keywords": [
		"workflow",
		"middleware",
		"jexl",
		"expression"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vadolasi/refluxo.git",
		"directory": "packages/jexl-middleware"
	},
	"homepage": "https://github.com/vadolasi/refluxo-engine/tree/main/packages/jexl-middleware#readme",
	"bugs": {
		"url": "https://github.com/vadolasi/refluxo-engine/issues"
	},
	"author": "Vitor Daniel <vitor036daniel@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"jexl": "^2.3.0"
	},
	"peerDependencies": {
		"@refluxo/core": "workspace:*"
	},
	"devDependencies": {
		"@refluxo/core": "workspace:*",
		"@types/jexl": "^2.3.4",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3"
	}
}
